<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="aplikacja.strony.navigacja.stronanav2"
             Title="stronanav2"
             BackgroundColor="green">
    <VerticalStackLayout>
        <Label 
            Text="To jest strona 2"
            FontSize="20"
            FontAttributes="Bold"
            Margin="20"
            TextColor="white"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button x:Name="navstrona3"
            Text="strona3"
            Margin="20"
            BackgroundColor="Blue"
            TextColor="red"
            Clicked="navstrona3_click"></Button>
        <Button x:Name="closebutton"
            Text="Click Me"
            Margin="20"
            BackgroundColor="Blue"
            TextColor="red"
            Clicked="closebutton_click"></Button>
    </VerticalStackLayout>
</ContentPage>